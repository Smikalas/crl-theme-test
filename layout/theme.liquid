<script>
  {{ 'video-scroll.js' | asset_url | script_tag }}
</script>

<style>
.video-section {
  position: relative;
  height: 100vh;
  overflow: hidden;
  background: #000;
}

.video-scroll {
  position: absolute;
  top: 50%;
  left: 50%;
  width: 100%;
  height: 100%;
  object-fit: cover;
  transform: translate(-50%, -50%);
}

#videoscrolls {
  height: 500vh;
  position: relative;
  max-width: 100%;
  margin: 0;
  padding: 0;
}
</style>

<section class="video-section">
  <div id="videoscrolls">
    <video 
      src="https://cdn.shopify.com/videos/c/o/v/d22c89546d5f40d3921c613514742087.mp4" 
      playsinline 
      webkit-playsinline 
      preload="auto" 
      muted 
      class="video-scroll"
    ></video>
  </div>
</section>

</html>
